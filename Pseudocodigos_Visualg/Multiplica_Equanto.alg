algoritmo "semnome"

var
   numero1, numero2, resultado, mul:Inteiro
inicio
      Escreva("1°numero:")
      leia(numero1)
      Escreva("2°numero:")
      leia(numero2)
      mul<-0
      Enquanto mul < numero2 faca
                   mul <- mul + 1
                   resultado <- resultado + numero1
      FimEnquanto
      Escreval("Resultado: ", resultado)
fimalgoritmo
